{
  "rewrites": [
    {
      "source": "/public/:file*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ],
      "destination": "/public/:file*"
    },
    {
      "source": "/public/(.*)",
      "status": 403
    }
  ]
}
